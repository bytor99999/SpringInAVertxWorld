<section class="span9 offset1">
    <H1>Chat</H1>
    <section>
        <textarea id="chatTextArea" cols="300" readonly rows="30" class="span9"></textarea>
    </section>
    <br>

    <section>
        <form id="chatForm" class="form-horizontal">
        <input id="message" type="text"
               class="input-large span8" placeholder="Message"
               name="message"
               value="">
        <button id="sendChat" type="submit" class="btn btn-primary btn-large">Send</button>
        </form>
    </section>
</section>